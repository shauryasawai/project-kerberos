{
  "name": "project-kerberos",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prettier": "prettier --write .",
    "format": "prettier --write --config .prettierrc --ignore-path .prettierignore .",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0 --fix",
    "format:check": "prettier --check --config .prettierrc --ignore-path .prettierignore .",
